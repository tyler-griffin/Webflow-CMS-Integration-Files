{
  "manifest_version": 3,
  "name": "Cybflow",
  "version": "1.0",
  "description": "A simple extension to help add cybdata/cybkey attributes.",
  "permissions": [ "contextMenus", "clipboardRead", "clipboardWrite", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
